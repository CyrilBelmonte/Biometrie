<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Ellipse?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="277.0" prefWidth="600.0"
            style="-fx-background-color: #9e9e9e; -fx-border-color: #eeeeee; -fx-border-width: 2px;"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ihm.controller.BiometricVerificationController">
   <children>
      <Button fx:id="button" layoutX="331.0" layoutY="194.0" mnemonicParsing="false" onAction="#startCamera"
              text="Prendre la photo">
         <font>
            <Font name="Comic Sans MS" size="18.0"/>
         </font>
      </Button>
      <Text layoutX="280.0" layoutY="91.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Veuillez vous placer au centre "
            wrappingWidth="347.8125">
         <font>
            <Font name="Comic Sans MS" size="24.0"/>
         </font>
      </Text>
      <Text layoutX="280.0" layoutY="124.0" strokeType="OUTSIDE" strokeWidth="0.0" text="de l'image dans l'ellipse">
         <font>
            <Font name="Comic Sans MS" size="24.0"/>
         </font>
      </Text>
      <ImageView fx:id="cameraImage" fitHeight="166.0" fitWidth="200.0" layoutX="38.0" layoutY="34.0"
                 pickOnBounds="true" preserveRatio="true"/>
      <Ellipse fill="#85b1da00" layoutX="138.0" layoutY="110.0" radiusX="59.0" radiusY="72.0" stroke="#d14c4c"
               strokeType="INSIDE"/>
   </children>
</AnchorPane>
